<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".sign_up.SignUpActivity">
    <TextView
        android:id="@+id/signUpTitle"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="31dp"
        android:layout_marginLeft="31dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="341dp"
        android:layout_marginRight="341dp"
        android:layout_marginBottom="15dp"
        android:text="회원가입"

        />

    <View
        android:id="@+id/view2"
        android:layout_width="380dp"
        android:layout_height="3px"

        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginBottom="8dp"
        android:background="#6B6FAE"
        />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="87dp"
        android:layout_height="81dp"
        android:layout_marginStart="162dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="50dp"
        android:layout_marginRight="162dp"
        app:srcCompat="@drawable/sungshin_logo" />

    <EditText
        android:id="@+id/et_id"
        android:layout_width="370dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="학번"
        android:inputType="textPersonName"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="150dp"
        android:layout_marginRight="162dp"
        />

    <EditText
        android:id="@+id/et_name"
        android:layout_width="370dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="이름"
        android:inputType="textPersonName"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="200dp"
        android:layout_marginRight="162dp"
        />

    <EditText
        android:id="@+id/et_pass"
        android:layout_width="370dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="비밀번호"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="250dp"
        android:layout_marginRight="162dp"
        />

    <EditText
        android:id="@+id/et_entYear"
        android:layout_width="370dp"
        android:layout_height="50dp"
        android:layout_marginBottom="4dp"
        android:ems="10"
        android:hint="입학 연도"
        android:inputType="textPersonName"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="300dp"
        android:layout_marginRight="162dp"
        />

    <EditText
        android:id="@+id/et_toeic"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="점수"
        android:inputType="textPersonName"
        android:layout_marginStart="110dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="570dp"
        android:layout_marginRight="162dp"
        />

    <Button
        android:id="@+id/btn_signUp"
        android:layout_width="369dp"
        android:layout_height="42dp"
        android:text="완료"
        app:backgroundTint="@color/mainColor"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="650dp"
        android:layout_marginRight="162dp"
        />


    <TextView
        android:id="@+id/tv_Academic"
        android:layout_width="33dp"
        android:layout_height="28dp"
        android:background="@null"
        android:text="학적"
        android:textColor="#040202"
        android:textSize="16sp"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="370dp"
        android:layout_marginRight="162dp"
        />
    <TextView
        android:layout_width="33dp"
        android:layout_height="28dp"
        android:background="@null"
        android:text="학년"
        android:textColor="#040202"
        android:textSize="16sp"
        android:layout_marginStart="190dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="370dp"
        android:layout_marginRight="162dp"
        />
    <TextView
        android:layout_width="33dp"
        android:layout_height="28dp"
        android:background="@null"
        android:text="학기"
        android:textColor="#040202"
        android:textSize="16sp"
        android:layout_marginStart="320dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="370dp"
        android:layout_marginRight="162dp"
        />

    <TextView
        android:id="@+id/tv_2ndMajorClass"
        android:layout_width="85dp"
        android:layout_height="27dp"
        android:background="@null"
        android:text="복부전 여부"
        android:textColor="#040202"
        android:textSize="16sp"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="420dp"
        android:layout_marginRight="162dp"
        />

    <TextView
        android:id="@+id/tv_mainMajorTitle"
        android:layout_width="48dp"
        android:layout_height="26dp"
        android:background="@null"
        android:text="주전공"
        android:textColor="#040202"
        android:textSize="16sp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="470dp"
        android:layout_marginRight="162dp"
        />

    <TextView
        android:id="@+id/tv_2ndMajor"
        android:layout_width="72dp"
        android:layout_height="30dp"
        android:background="@null"
        android:text="선택 전공"
        android:textColor="#040202"
        android:textSize="16sp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="520dp"
        android:layout_marginRight="162dp"
        />

    <TextView
        android:id="@+id/tv_toeic"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:background="@null"
        android:text="토익점수"
        android:textColor="#040202"
        android:textSize="16sp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="580dp"
        android:layout_marginRight="162dp"
        />

    <TextView
        android:id="@+id/tv_thesis"
        android:layout_width="68dp"
        android:layout_height="24dp"
        android:background="@null"
        android:text="졸업논문"
        android:textColor="#040202"
        android:textSize="16sp"
        android:layout_marginStart="215dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="580dp"
        android:layout_marginRight="162dp"
        />

    <Spinner
        android:id="@+id/sp_schoolYear"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:entries="@array/schoolYear"
        android:layout_marginStart="110dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="355dp"
        android:layout_marginRight="162dp"
        />

    <Spinner
        android:id="@+id/sp_semester"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:entries="@array/semester"
        android:layout_marginStart="240dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="355dp"
        android:layout_marginRight="162dp"
        />

    <Spinner
        android:id="@+id/sp_mainMajor"
        android:layout_width="260dp"
        android:layout_height="50dp"
        android:entries="@array/major"
        android:layout_marginStart="115dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="405dp"
        android:layout_marginRight="162dp"
        />

    <Spinner
        android:id="@+id/sp_2ndMajorClass"
        android:layout_width="170dp"
        android:layout_height="50dp"
        android:entries="@array/_2ndMajorClass"
        android:layout_marginStart="115dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="450dp"
        android:layout_marginRight="162dp"
        />

    <Spinner
        android:id="@+id/sp_2ndMajor"
        android:layout_width="260dp"
        android:layout_height="50dp"
        android:entries="@array/major"
        android:layout_marginStart="115dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="505dp"
        android:layout_marginRight="162dp"
        />

    <Spinner
        android:id="@+id/sp_thesis"
        android:layout_width="105dp"
        android:layout_height="44dp"
        android:entries="@array/thesis"
        android:layout_marginStart="280dp"
        android:layout_marginLeft="162dp"
        android:layout_marginEnd="162dp"
        android:layout_marginTop="570dp"
        android:layout_marginRight="162dp"
        />

</FrameLayout>

